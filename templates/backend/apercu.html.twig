{% extends 'baseBackend.html.twig' %}

{% block title %}
	{{ parent() }}





{% endblock %}


{% block stylesheets %}
	{{ parent() }}

	<link rel="stylesheet" href={{asset("css/article-show.css")}}>
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sceditor/2.1.3/themes/content/default.min.css" integrity="sha512-HFIO3TSewWJuQyXDMtoX7CvpEKMrkduO8j5dKPeF9smntHrFw402Jyy3CllTTpwonVp792hg/kp1iOK+85QCNQ==" crossorigin="anonymous" />
{% endblock %}

{% block stylesheetsspan %}
{{ parent() }}
{% endblock %}

{% block contenu %}
<body>
	<div class="container-fluid pb-2 mb-2">
		<div class="row">
			<div class="col-12 text-center p-0 m-0">
				<img src="https://zupimages.net/up/20/50/lsxc.jpg" alt="" class="img-fluid">
			</div>
		</div>
	</div>

	<!-- Page Content -->
	<div class="container bodyblog">

		<div
			class="row">

			<!-- Blog Entries Column -->
			<div
				class="col-lg-8">

				<!-- Title -->
				<h1 class="mt-4">
					{% if titreAjout is defined %}


					{{ titreAjout | raw }}
					{{ titre | raw }}
					{% else %}

					Champ non rempli
					{% endif %}
				</h1>

				<!-- Author -->
				<p class="lead">
					par
					<a href="#">
						Wedding Yourself
					</a>
				</p>

				<hr>

			<h6 class="mt-4">
					Publi√© le 
					{% if date is defined %}
							{{ date | format_datetime(locale='en', pattern='dd MMMM') }}
					{% endif %}
				</h6>

				<hr>
				<div id="apercuShow" name="apercu" class="container-fluid"> 
					
					{% if titreAjout is defined %}


					{{ contenuAjout | raw }}
					{{ contenu | raw }}
					{% else %}

					Champ non rempli
					{% endif %}
					
					
					

				</div>
				<hr>
			
			</div>

			<div class="col-lg-8"></div>


			<!-- Mettre Widget Aussi-->
			<div class="col-lg-4">
			
			</div>
		</div>
		<!-- /.row -->

	</div>
	<!-- /.container -->
</body>
	<!-- Fin corp du blog -->
{% endblock %}
